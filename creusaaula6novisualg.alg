algoritmo "creusa"
                         //dar tab
var
   ano,idade,nasci : inteiro
inicio
      escreva ("em que ano nos estamos? ")
      leia(ano)
      escreva ("em que ano eu nasci? ")
      leia(nasci)
      idade <- ano - nasci
      escreva ("creusa: vou precisar de ",idade," velas")

fimalgoritmo
