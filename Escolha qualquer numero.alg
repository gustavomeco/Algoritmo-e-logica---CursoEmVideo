algoritmo "0ateusuarioquiser"
var
   N,c,salto : inteiro
inicio
      Escreva("Escolha qualquer numero: ")
      leia(N)
      Escreva("de quanto em quanto: ")
      leia(salto)
      c <- 0
      enquanto (c <= N) faca
      Escreval(c)
      c <- c + salto
      fimenquanto
fimalgoritmo
