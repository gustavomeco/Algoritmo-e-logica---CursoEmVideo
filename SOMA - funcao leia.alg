algoritmo "semnome"
var
   n1,n2,soma1: inteiro
funcao soma(a,b:inteiro) :inteiro
inicio
      retorne a + b
fimfuncao
inicio
      Escreva("DIGITE UM VALOR: ")
      leia(n1)
      Escreva("DIGITE UM VALOR: ")
      leia(n2)
      soma1 <- soma(n1,n2)
      Escreval("O VALOR DA SOMA DE",n1," +",n2," EH ",soma1)
fimalgoritmo
