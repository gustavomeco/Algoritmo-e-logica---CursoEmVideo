algoritmo "cadastro em site "
var
 id: inteiro
 nome,cargo: caractere
 ver: logico
inicio
 Escreva("digite seu nome: ")
 leia (nome)
 Escreva("digite seu cargo empresarial: ")
 leia (cargo)
 Escreva("digite sua idade: ")
 leia (id)
 ver <- id >= 18
 Escreval("voce pode cadastrar no sistema? ",ver)
fimalgoritmo
